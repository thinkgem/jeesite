<?xml version="1.0" encoding="UTF-8"?>
<ehcache updateCheck="false" name="customCache">

	<diskStore path="java.io.tmpdir/jeesite/ehcache/custom" />

	<!-- DefaultCache setting. -->
	<defaultCache maxEntriesLocalHeap="10000" eternal="false" timeToIdleSeconds="300" timeToLiveSeconds="600"
		overflowToDisk="true" maxEntriesLocalDisk="100000" />

   	<!-- SYS cache. -->
	<cache name="sysCache" maxEntriesLocalHeap="10000" eternal="true" timeToIdleSeconds="0" timeToLiveSeconds="0" overflowToDisk="true" />
	
	<!-- CMS cache. -->
	<cache name="cmsCache" maxEntriesLocalHeap="10000" eternal="true" timeToIdleSeconds="0" timeToLiveSeconds="0" overflowToDisk="true" />
		
</ehcache>
