		overflowToDisk="true" maxEntriesLocalDisk="100000" >
		<cacheEventListenerFactory class="net.sf.ehcache.distribution.RMICacheReplicatorFactory"
			properties="replicatePuts=false,replicateUpdatesViaCopy=false" />
    </defaultCache>

